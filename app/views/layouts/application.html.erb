<!DOCTYPE html>
<html>
<head>
  <title>YouClip</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body>
	
	<div class="container">
		
		<div class="row top-row">
			<div class="col-md-10 col-md-offset-1">
				<nav class="navbar navbar-default navbar-embossed" role="navigation">
					<div class="navbar-header">
						<a class="navbar-brand" href="<%= root_path %>">YouClip</a>
					</div>
				</nav>
			</div>	
		</div>
		
		<div class="row youclip-title-row">
			<div class="col-md-12">
				<h1 class="text-center">YouClip</h1>
			</div>
		</div>
		
		<div class="row">
			<div class="col-md-10 col-md-offset-1"><%= yield %></div>
		</div>
	
	</div>

</body>
</html>
