<%= javascript_tag do %>

	function onPlayerReady(event){
        playClip();
    }
    
	function playClip(){
		player.setPlaybackQuality('hd720');
		player.loadVideoById({
			videoId:'<%= @clip.youtube_id %>', 
			startSeconds:<%= @clip.start %>, 
			endSeconds:<%= @clip.end %>
		});
    }
    
<% end %>